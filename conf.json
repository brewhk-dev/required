{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "plugins": ["plugins/markdown"],
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "dateFormat": "DD MMM YYYY",
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "systemName": "Required",
    "footer": "",
    "copyright": "Copyright Â© 2015 Brew Creative Limited.",
    "navType": "vertical",
    "theme": "spacelab",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true,
    "highlightTutorialCode": true,
    "protocol": "html://"
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}